{
  "compilerOptions": {
    "target": "es5",
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": true,
    "module": "commonjs",
    "experimentalDecorators": true,
    "outDir": "out",
    "baseUrl": "./src",
    "lib": [
        "dom",
        "es6",
        "dom.iterable",
        "scripthost",
        "es2016.array.include"
    ],
    "types": [
      "bootstrap",
      "jasmine",
      "node"
    ]
  },
  "exclude": [
    "out",
    "node_modules",
    "dist",
    "dev",
    "reports"
  ],
  "version": "1.7.5"
}
